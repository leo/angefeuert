{{#slick-slider autoplay=true autoplaySpeed=5000 speed=2000 arrows=false dots=true fade=true}}
  {{#each model as |index|}}
    <div class="box">
      <picture>
        <source srcset="images/slide-{{index}}-md.png 1x, images/slide-{{index}}-md@2x.png 2x" media="(min-width: 992px)">
        <source srcset="images/slide-{{index}}-sm.png 1x, images/slide-{{index}}-sm@2x.png 2x" media="(min-width: 768px)">
        <source srcset="images/slide-{{index}}@2x.png">

        <img src="images/slide-{{index}}@2x.png">
      </picture>
    </div>
  {{/each}}
{{/slick-slider}}

<aside class="logo-big">
  {{inline-svg "images/logo-big.svg"}}
</aside>

<a href="http://www.tulikivi.com/de" target="_blank" class="copyright">Foto: <b>Tulikivi</b></a>
